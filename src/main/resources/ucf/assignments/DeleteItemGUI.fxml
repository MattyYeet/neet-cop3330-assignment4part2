<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>

<AnchorPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.DeleteItemController">
    <ToolBar prefHeight="40.0" prefWidth="600.0">
        <Label text="Which item would you like to delete?" />
        <TextField fx:id="searchDelete" promptText="Search" translateX="125.0" />
        <Button fx:id="DeleteYes" mnemonicParsing="false" onAction="#remove" text="Delete" translateX="125.0" />
      <Button fx:id="nevermind" mnemonicParsing="false" onAction="#cancel" text="Cancel" translateX="125.0" />
    </ToolBar>
    <TableView layoutY="40.0" prefHeight="361.0" prefWidth="600.0">
        <columns>
            <TableColumn prefWidth="513.0" text="Items" />
            <TableColumn prefWidth="86.0" text="Completed?" />
        </columns>
    </TableView>
    <Line endX="148.0" endY="39.0" layoutX="366.0" layoutY="361.0" startX="148.0" startY="-321.0" />
</AnchorPane>
